#!/usr/bin/env mkb
# Builder mkb file for the java portion of the s3eFlurry extension on android
platform JAVA

files
{
    (source/android)
    s3eFlurry.java
}

librarypath "$MARMALADE_ROOT/extensions/s3eFlurry/lib/android/libFlurry/FlurryAgent.jar"
librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"

option output-name=lib/android/s3eFlurry.jar
